<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ragroup.partner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ragroup.partner.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ragroup.partner.UserActivity"
            android:label="@string/title_activity_user" >
        </activity>
        <activity
            android:name="com.ragroup.partner.ProductActivity"
            android:label="@string/title_activity_product" >
        </activity>
        <activity
            android:name="com.ragroup.partner.PromotionActivity"
            android:label="@string/title_activity_promotion" >
        </activity>
    </application>

</manifest>
